<%= form_for priority, :remote=>true, 'data-disable-with' => escape_t(t(:ajax_tagging)), :url => tag_save_priority_path(priority) do |f| %>
	<%= f.text_field :issue_list, :id => 'priority_' + priority.id.to_s + '_issue_list', :size => 70 %>
	<%= f.submit t('endorsements.save'), :class => "tag_button" %>

<% end %>
