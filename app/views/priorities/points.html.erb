<%= render :partial => "header" %>
<%= render :partial => "nav" %>
<p>
<% if @questions.empty? and not @priority.replaced? %>
	<%= t('points.empty') %>
<% else %>
	<% if @questions.total_pages > 1 %>
	<%= will_paginate(@questions) %>
	<p>
	<% end %>
	<% for point in @questions %>
		<% quality = nil %>
		<% if @qualities %>
			<% quality = @qualities.detect {|e| e.question_id == point.id } %>
		<% end %>				
		<%= render :partial => "questions/show", :locals => {:point => point, :priority => @priority, :quality => quality }%>
	<% end %>
	<%= will_paginate(@questions) %>
<% end %>
