<% if priority.replaced? %>
	<div id="hp_intro"><strike><%= h(priority.name)%></strike></div>
<% else %>
	<div id="hp_intro" class="priorities"><%= link_to_unless_current h(priority.name), priority %> <span class="box smallbox"><%=priority.cached_allocated_points_counter%> punktar</span></div>
<% end %>