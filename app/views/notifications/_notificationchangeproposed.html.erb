<%= tr("{sender_name} proposed {new_priority_name} acquire {priority_name}. <a href='{change_url}'>Please review</a>.", "view/notifications/_notificationchangeproposed", :sender_name => link_to(notification.sender.name,notification.sender), :new_priority_name => link_to(notification.notifiable.new_priority.name, notification.notifiable.new_priority), :priority_name => link_to(notification.notifiable.priority ? notification.notifiable.priority.name : "deleted", notification.notifiable.priority), :change_url => priority_change_url(notification.notifiable.priority,notification.notifiable)) if notification.notifiable.priority %>