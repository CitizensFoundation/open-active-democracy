<link rel="icon" href="<%= current_government.fav_icon.url(:icon_16) %>">
<% if request.subdomains.any? and request.subdomains[0] != 'www' %>
	<link rel="canonical" href="http://<%= current_government.base_url %><%= url_for %>" />
<% end %>
<% if @rss_url and @page_title %>
	<link rel="alternate" type="application/rss+xml" title="<%= @page_title %>" href="<%= @rss_url %>">
<% end %>
<link rel="alternate" type="application/rss+xml" title="<%= tr("Top 25 priorities at {government_name}", "view/shared/_head", :government_name => tr(current_government.name,"Name from database")) %>" href="<%= url_for :controller => "priorities", :action => "top", :format => "rss", :only_path => false %>">