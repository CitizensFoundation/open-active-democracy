<div id="branding" style="height: 85px">
  <table>
    <tr>
      <td>
        <% if current_partner %>
            <div id="partner_logo">
              <%= render :partial => "partners/logo" %>
            </div>
        <% else %>
            <div id="main_logo">
              <%= link_to :controller=>"portal", :host=>request.host_with_port do %>
                  <%= image_tag "/images/new/new_logo.png"%>
              <% end %>
            </div>
        <% end %>
      </td>
    </tr>
  </table>
</div>
