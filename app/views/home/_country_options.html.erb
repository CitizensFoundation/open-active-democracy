<% cache(["country_options",locale,Partner.current_id,@inline_translations_enabled], :expires_in => 5.seconds) do %>
    <div class="all_countries">
      <table>
        <tr>
          <% all_countries_in_columns.each do |column| %>
              <td align="center">
                <% column.each do |country| %>
                    <% if partner = Partner.find_by_iso_country_id(country.id) %>
                        <div class="country">
                          <%= link_to partner.show_url do %>
                              <%= image_tag country.medium_flag_image, :size=>"32x32", :border=>0 %><br>
                              <%=country.country_english_name%>
                          <% end %>
                        </div>
                    <% end %>
                <% end %>
              </td>
          <% end %>
        </tr>
      </table>
    </div>
<% end %>
